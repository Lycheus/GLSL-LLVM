<?xml version="1.0" encoding="US-ASCII"?>
<!-- This tests if the GLSL compiler implementation supports the comma       -->
<!-- operator.                                                               -->
<!-- The fragment shader should fail to compile.                             -->
<test mode="gl" pattern="build">
	<compstat>false</compstat>
	<linkstat>false</linkstat>
	<shader>
		<vertshader>../default/default.vert</vertshader>
		<fragshader>comma2_frag.frag</fragshader>
	</shader>
</test>
