<?xml version="1.0" encoding="US-ASCII"?>
<!-- This tests the behavior when the function definition parameters         -->
<!-- qualifiers are different from the function declaration parameter        -->
<!-- qualifiers.                                                             -->
<!-- The fragment shader should fail to compile.                             -->
<test mode="gl" pattern="build">
	<compstat>false</compstat>
	<linkstat>false</linkstat>
	<shader>
		<vertshader>../default/default.vert</vertshader>
		<fragshader>function9_frag.frag</fragshader>
	</shader>
</test>
