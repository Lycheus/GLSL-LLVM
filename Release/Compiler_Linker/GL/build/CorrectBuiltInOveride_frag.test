<?xml version="1.0" encoding="US-ASCII"?>
<!-- This tests if the GLSL compiler implementation allows built in          -->
<!-- functions to be overriden.                                              -->
<!-- The fragment shader should compile.                                     -->
<test mode="gl" pattern="build">
	<compstat>true</compstat>
	<linkstat>true</linkstat>
	<shader>
		<vertshader>../default/default.vert</vertshader>
		<fragshader>CorrectBuiltInOveride_frag.frag</fragshader>
	</shader>
</test>
