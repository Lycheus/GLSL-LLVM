<?xml version="1.0" encoding="US-ASCII"?>
<!-- This tests if the GLSL compiler implementation supports extentions      -->
<!-- correctly.                                                              -->
<!-- The fragment shader should compile.                                     -->
<test mode="gl" pattern="build">
	<compstat>true</compstat>
	<linkstat>true</linkstat>
	<shader>
		<vertshader>../default/default.vert</vertshader>
		<fragshader>CorrectExtension4_V100_frag.frag</fragshader>
	</shader>
</test>
