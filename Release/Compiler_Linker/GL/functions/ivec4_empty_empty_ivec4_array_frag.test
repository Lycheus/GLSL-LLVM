	<shader>
		<vertshader>../default/default.vert</vertshader>
		<fragshader>../default/expected.frag</fragshader>
		<uniform>
			<name>result</name>
			<uniform4fv>1.0 1.0 1.0 1.0</uniform4fv>
		</uniform>
	</shader>
</test>
