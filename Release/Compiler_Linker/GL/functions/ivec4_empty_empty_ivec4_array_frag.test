<!-- parameter type = ivec4                                                  -->
<!-- array specifier = array                                                 -->
<test mode="gl" pattern="compare">
	<shader>
		<vertshader>../default/default.vert</vertshader>
		<fragshader>ivec4_empty_empty_ivec4_array_frag.frag</fragshader>
